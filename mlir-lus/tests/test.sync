sync.node @aux(%i: !sync.sigin<i32>) -> (%o: !sync.sigout<i32>) {
  %z = sync.input(%i): i32
  sync.output(%o:%z): i32
  sync.end
}

sync.node @test (%i: !sync.sigin<i32>) -> (%o: !sync.sigout<i32>) {
  %a = sync.input(%i): i32
  %b = sync.inst @aux 2 (%a) : (i32) -> i32
  // sync.output(%o:%b): i32
  sync.output(%o:%a): i32
  sync.end
}
